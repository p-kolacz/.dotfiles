{
	"telemetry.enableTelemetry": false,
	"telemetry.enableCrashReporter": false,
	"workbench.enableExperiments": false,
	"workbench.settings.enableNaturalLanguageSearch": false,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.colorTheme": "Plastic",
	"editor.fontLigatures": true,
	"window.zoomLevel": 1,
	"git.enableSmartCommit": true,
	"files.autoSave": "onWindowChange",
	"editor.fontFamily": "'UbuntuMono Nerd Font', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.fontSize": 15,
	"keyboard.dispatch": "keyCode",
	"files.simpleDialog.enable": true,
	"files.associations": {
		".xbindkeysrc": "shellscript",
		"vimrc": "viml",
		"vifmrc": "viml",
		"*.conf": "shellscript"
	},
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "none",
	"html.autoClosingTags": false,
	"indentRainbow.colors": [
		"rgba(16,16,16,0.1)",
		"rgba(16,16,16,0.2)",
		"rgba(16,16,16,0.3)",
		"rgba(16,16,16,0.4)",
		"rgba(16,16,16,0.5)",
		"rgba(16,16,16,0.6)",
		"rgba(16,16,16,0.7)",
		"rgba(16,16,16,0.8)",
		"rgba(16,16,16,0.9)",
		"rgba(16,16,16,1.0)"
	],
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.insertSpaces": false,
	"editor.smoothScrolling": true,
	// Macros
	"macros": {
		"commentLine": [
			"editor.action.commentLine",
			"cursorDown"
		],
		"commentDown": [
			"editor.action.copyLinesDownAction",
			"cursorUp",
			"editor.action.addCommentLine",
			"cursorDown"
		],
	},
	// Vim
	"vim": {
		"useSystemClipboard": true,
		"easymotion": true,
		"insertModeKeyBindings": [
			{
				"before": [
					"j",
					"j"
				],
				"after": [
					"<Esc>"
				]
			},
		],
		"vim.normalModeKeyBindings": [

		],
		"normalModeKeyBindingsNonRecursive": [
			{
				"before": [
					"<CR>"
				],
				"after": [
					"o",
					"<Esc>"
				]
			},
			{
				"before": [
					"Z",
					"Z"
				],
				"commands": [
					":wq"
				]
			},
			{
				"before": [
					"<Space>"
				],
				"after": [
					":"
				]
			}
		],
	},
	"editor.lineNumbers": "relative",
	"gitlens.advanced.telemetry.enabled": false,
	"gitlens.currentLine.enabled": false,
	"gitlens.codeLens.recentChange.enabled": false,
	"gitlens.codeLens.authors.enabled": false,
	"explorer.openEditors.visible": 0,
	"GodotTools.editorPath": "/usr/bin/godot",
	"GodotTools.lint": {
		"semicolon": false,
		"conditionBrackets": false,
		"unusedSymbols": false
	},
	"php.suggest.basic": false
}